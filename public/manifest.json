{
  "manifest_version": 3,
  "name": "Query X Extension",
  "version": "1.0.2",
  "description": "Query X Extension",
  "permissions": ["activeTab", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["<all_urls>"],
  "externally_connectable": {
    "matches": ["*://*/*"]
  },
  "icons": {
    "48": "vite.svg"
  }
}
